% first individual has 6 1's and 4 0's
%this code graphs the negative log likelyhood of individual #1
%with respect to alpha and gamma and finds alpha and gamma
%for which negative log likelyhood is minimazied



function [] = negative_likelihood(A,start,step,stop)

%initialize variables


[alpha,gamma] = meshgrid(start:step:stop);
alpha_min_total = zeros(length(A)/10,1);
gamma_min_total = zeros(length(A)/10,1);
min_total = zeros(length(A)/10,1);
k = 0


for i = 1:10:length(A);
    
    num_ones = 0;
    first_sum = 0;
    second_sum = 0;
    log_P = zeros((stop-start)/step + 1);
    
    for j = i:(i+9);
        
        if A(j) == 1;
            num_ones = num_ones + 1;
        end
    end
        
    

    for p = 0.1:0.1:(num_ones/10);
    
        %model utility and probability of choosing safe option
        U_X = real((exp(-(log(p)).^gamma)*(100.^alpha) + (1-exp(-(log(p)).^gamma))*(80.^alpha)));

        U_Y = real((exp(-(log(p)).^gamma)*(190.^alpha) + (1-exp(-(log(p)).^gamma))*(5.^alpha)));

        P = 1./(1+exp(U_Y-U_X));
    
    
        % set threshold to avoid negative log of probability to be
        % extremely large
        for i = 1:((stop-start)/step + 1);
            for j = 1:((stop-start)/step + 1);
                if P(i,j) > 10^(-80);
                log_P(i,j) = log(P(i,j));
                else
                log_P(i,j) = -80;
                end
            end
        end
            
   
    % compute the sum of negative log likelyhoods the produce output=1 
    first_sum = first_sum - log_P;
    
    end


    for p = (num_ones/10 + 0.1):0.1:1;
    
        %model utility and probability of choosing safe option
        U_X = real((exp(-(log(p)).^gamma)*(100.^alpha) + (1-exp(-(log(p)).^gamma))*(80.^alpha)));

        U_Y = real((exp(-(log(p)).^gamma)*(190.^alpha) + (1-exp(-(log(p)).^gamma))*(5.^alpha)));

        P = 1./(1+exp(U_Y-U_X));
    
        % compute the sum of negative log likelyhoods the produce output=0
        
        second_sum = second_sum - (1-log_P);
    
    
    end


    neg_log_like = first_sum + second_sum;


    %find alpha and gamma for which negative log likelyhood is minimized

    min = 700;
    for i = 1:((stop-start)/step + 1);
        for j = 1:((stop-start)/step + 1);
            if neg_log_like(i,j) < min;
                min = neg_log_like(i,j);
                alpha_min = (j-1)/100;
                gamma_min = (i-1)/100;
            end
        end
    end
    
    %disp(min);
    
    
    
    k = k + 1;
     
    alpha_min_total(k,1) = alpha_min;
    gamma_min_total(k,1) = gamma_min;
    min_total(k,1) = min;
         
    
    
    
    
end


disp(gamma_min_total);

%disp(alpha_min_total);

%disp(min_total);





end





    




